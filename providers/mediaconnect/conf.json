{
  "name": "MediaConnect",
  "desc": "MediaConnect is something",
  "url": "https://login.mediaconnect.no/oauth",
  "oauth2": {
    "authorize": "/authorize",
    "access_token": "/token",
    "request": {
      "url": "https://api.mediaconnect.no/capi",
      "cors": false,
      "query": {
        "access_token": "{{token}}"
      },
      "headers": {
        "Accept": "application/json"
      }
    },
    "refresh": "/token",
    "parameters": {
      "client_secret": "string",
      "client_id": "string",
      "scope": {}
    }
  },
  "href": {
    "docs": "https://doc.mediaconnect.no/doc/ConnectID/index.html",
    "provider": "http://www.mediaconnect.no/"
  }
}
